{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfnetflixprojj"
		},
		"NetflixAzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'NetflixAzureDataLakeStorage'"
		},
		"githubsinkcopy_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'githubsinkcopy'"
		},
		"githubcopysink_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'githubcopysink'"
		},
		"NetflixAzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://netflixprojdlake.dfs.core.windows.net/"
		},
		"NetflixHttpServerLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"githubsinkcopy_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://netflixprojdlake.dfs.core.windows.net/"
		},
		"githubcopysink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://netflixprojdlake.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfnetflixprojj_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"NetflixAzureDataLakeStorage_accountKey": {
						"value": "[parameters('NetflixAzureDataLakeStorage_accountKey')]"
					},
					"githubsinkcopy_accountKey": {
						"value": "[parameters('githubsinkcopy_accountKey')]"
					},
					"githubcopysink_accountKey": {
						"value": "[parameters('githubcopysink_accountKey')]"
					},
					"NetflixAzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('NetflixAzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"NetflixHttpServerLS_properties_typeProperties_url": {
						"value": "[parameters('NetflixHttpServerLS_properties_typeProperties_url')]"
					},
					"githubsinkcopy_properties_typeProperties_url": {
						"value": "[parameters('githubsinkcopy_properties_typeProperties_url')]"
					},
					"githubcopysink_properties_typeProperties_url": {
						"value": "[parameters('githubcopysink_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}